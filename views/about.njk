{% extends "layout.njk" %}

{%block content%}

  <div id="wrapper">
    <img src="{{about.url}}" alt=""/>
    <h1>{{about.name}}</h1>
    <h2>{{about.title}}</h2>
    <p>
      {{about.description}}
    </p>
  </div>

  <div id="links_footer" class="links">
    {% for link in about.links %}

      <a href="{{link.url}}" target="_blank">{{link.name}}</a>
    {% endfor %}
  </div>

{% endblock %}

</body>
</html>